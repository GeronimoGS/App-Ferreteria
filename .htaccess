RewriteRule ^productos([0-9]+)$ controllers/listaProductos.php?c=$1

RewriteRule ^detalle$ controllers/detalleproducto.php

RewriteRule ^claveproduc$ controllers/ClaveAccesoAltaProductos.php

RewriteRule ^modificarcliente$ controllers/ModificarCliente.php

RewriteRule ^eliminarcliente$ controllers/EliminarCliente.php

RewriteRule ^eliminarproducto$ controllers/EliminarProducto.php

RewriteRule ^clave-modificar-producto$ controllers/ClaveModificarProducto.php

RewriteRule ^imprimir$ controllers/Imprimir.php

