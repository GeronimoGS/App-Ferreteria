# archivo .htaccess

RewriteEngine on

RewriteRule ^home$ controllers/Inicio.php

RewriteRule ^productos$ controllers/listaProductos.php

RewriteRule ^buscar$ controllers/Buscar.php

RewriteRule ^categorias$ controllers/listaCategorias.php

RewriteRule ^alta-producto$ controllers/AltaProducto.php

RewriteRule ^carrito$ controllers/carrito.php

RewriteRule ^carrito([0-9]+)$ controllers/carrito.php?id=$1

RewriteRule ^clientes$ controllers/listadoClientes.php

RewriteRule ^facturas$ controllers/ListadoFacturas.php

