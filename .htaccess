RewriteRule ^alta-categoria$ controllers/AltaCategoria.php

RewriteRule ^agregado-categoria$ controllers/AltaCategoriaOk.php

RewriteRule ^finalizar-compra$ controllers/Finalizarcompra.php

RewriteRule ^compras$ controllers/compras.php

